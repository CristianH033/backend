{
  "name": "Spring Boot DevContainer",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/app",
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",
        "pivotal.vscode-boot-dev-pack",
        "redhat.vscode-xml",
        "vscjava.vscode-maven"
      ]
    }
  },
  "remoteUser": "devuser",
  "forwardPorts": [8080]
}
